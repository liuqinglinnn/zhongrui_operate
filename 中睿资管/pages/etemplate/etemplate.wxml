      ></video>
    </view>
  </view>
  <view class="UpLoadPicture">

    <view class="UpLoadPicture_text">
      入库音频
    </view>
    <view class="UpLoadPicture_">
      <view class="UpLoadPicture_item">
        <view style="width:100%; display:flex;  justify-content: center; align-items: center;">
          <audio
            style="width:80%"
            bindpause="paused"
            data-ssrc="http://image.fzuhuahuo.cn/%E6%8A%A5%E8%AD%A6%E6%8A%A5%E5%A4%87%EF%BC%8C%E9%80%9A%E8%AF%9D%E8%AE%B0%E5%BD%95%E5%8F%82%E8%80%83%E9%9F%B3%E9%A2%91.mp3"
            bindplay="audioPlayed"
            name="报警报备，通话记录"
            src="http://image.fzuhuahuo.cn/%E6%8A%A5%E8%AD%A6%E6%8A%A5%E5%A4%87%EF%BC%8C%E9%80%9A%E8%AF%9D%E8%AE%B0%E5%BD%95%E5%8F%82%E8%80%83%E9%9F%B3%E9%A2%91.mp3"
            poster=""
            controls
          />
          <view style="position: absolute;right:20%">
            <view
              class=""
              bindtap="playaudio"
              data-src="http://image.fzuhuahuo.cn/%E6%8A%A5%E8%AD%A6%E6%8A%A5%E5%A4%87%EF%BC%8C%E9%80%9A%E8%AF%9D%E8%AE%B0%E5%BD%95%E5%8F%82%E8%80%83%E9%9F%B3%E9%A2%91.mp3"
              hidden="{{statustotal!=null}}"
            >
              播放
            </view>
            <view
              class=""
              bindtap="pauseaudio"
              data-src="http://image.fzuhuahuo.cn/%E6%8A%A5%E8%AD%A6%E6%8A%A5%E5%A4%87%EF%BC%8C%E9%80%9A%E8%AF%9D%E8%AE%B0%E5%BD%95%E5%8F%82%E8%80%83%E9%9F%B3%E9%A2%91.mp3"
              hidden="{{statustotal==null}}"
            >
              暂停
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
