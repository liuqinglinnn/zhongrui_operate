    </view>
  </view>
  <view class="UpLoadPicture">
    <view class="UpLoadPicture_text">
      入库视频
    </view>
    <view class="UpLoadPicture_">
      <video
        class="UpLoadPicture_img"
        src="http://image.fzuhuahuo.cn/%E5%85%A5%E5%BA%93%E8%B5%84%E6%96%99%E5%8F%82%E8%80%83%E8%A7%86%E9%A2%91.mp4"
